@startuml

!theme plain
top to bottom direction
skinparam linetype ortho

class Declencheur {
  + Declencheur(int, int): 
  - y: int
  - active: boolean
  - x: int
  + setActive(): void
  + etrePresent(int, int): boolean
   y: int
   x: int
   active: boolean
}
interface Entite << interface >> {
  + etrePresent(int, int): boolean
   y: int
   x: int
}
class LabyDessin {
  + LabyDessin(): 
  + dessinerJeu(Jeu, Canvas): void
}
class LabyJeu {
  + LabyJeu(): 
  + LabyJeu(String): 
  - laby: Labyrinthe
  + etreFini(): boolean
  + update(double, Clavier): void
  + init(): void
   laby: Labyrinthe
}
class Labyrinthe {
  + Labyrinthe(String): 
  + attaque(): void
  + etreFini(): boolean
  + getSuivant(int, int, String): int[]
  + deplacerPerso(String): void
  + deplacerMonstreAll(): void
  + getMur(int, int): boolean
   lengthY: int
   length: int
}
class Main {
  + Main(): 
  + main(String[]): void
}
class MainLaby {
  + MainLaby(): 
  + main(String[]): void
}
class Menu {
  + Menu(): 
  + listeFichiers(String): File[]?
  + start(Stage): void
}
class Monstre {
  + Monstre(int, int): 
  ~ y: int
  ~ x: int
  + subirAttaque(): void
  + etrePresent(int, int): boolean
  + etreMort(): boolean
   y: int
   x: int
}
class Perso {
  + Perso(int, int): 
  ~ x: int
  ~ y: int
  ~ enAttente: boolean
  + etrePresent(int, int): boolean
   y: int
   x: int
   enAttente: boolean
}
class Porte {
  + Porte(int, int): 
  - declencheur: Declencheur
  + etrePresent(int, int): boolean
   declencheur: Declencheur
}

LabyJeu     "1" *-[#595959,plain]-> "laby\n1" Labyrinthe  
Labyrinthe  "1" *-[#595959,plain]-> "Trigger\n*" Declencheur 
Labyrinthe  "1" *-[#595959,plain]-> "monstres\n*" Monstre     
Labyrinthe  "1" *-[#595959,plain]-> "pj\n1" Perso       
Labyrinthe  "1" *-[#595959,plain]-> "portes\n*" Porte       
Perso        -[#008200,dashed]-^  Entite      
Porte       "1" *-[#595959,plain]-> "declencheur\n1" Declencheur 
@enduml
